{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./",
    "skipDefaultLibCheck": true,
    "noEmitOnError": true,
    "incremental": true,
    "typeRoots": ["./types", "./node_modules/@types"]
  },
  "include": ["index.ts"],
  "exclude": ["node_modules"]
}
